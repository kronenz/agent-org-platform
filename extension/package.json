{
  "name": "agent-org-platform-extension",
  "version": "0.1.0",
  "description": "VS Code Extension for Agent Org Platform",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build --workspaces",
    "clean": "npm run clean --workspaces && rm -rf extension/out",
    "test": "npm run test --workspaces",
    "lint": "eslint . --ext .ts,.tsx",
    "dev": "npm run dev --workspaces"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
